	<queryString>
		<![CDATA[SELECT
    ESENZIONI_CITTADINO."NUM_PROTOCOLLO_SOGEI" NUM_PROTOCOLLO,
    ESENZIONI_CITTADINO."COD_NAZIONALE_ASL_RILASCIO" COD_ASL,
    ASL."DENOMINAZIONE" DESC_ASL,
    --DICHIARANTE."COGNOME" COGNOME_DICHIARANTE,
    --DICHIARANTE."NOME" NOME_DICHIARANTE,
    --TO_CHAR( DICHIARANTE."DATA_NASCITA", 'DD/MM/YYYY') DATA_NASCITA_DICHIARANTE,
    --COMUNE_DICHIARANTE."DENOMINAZIONE" COMUNE_NASCITA_DICHIARANTE,
    --COMUNE_DICHIARANTE."COD_PROVINCIA" PROVINCIA_NASCITA_DICHIARANTE,
    --DICHIARANTE."COD_FISCALE" COD_FISCALE_DICHIARANTE,
    TITOLO."DESCRIZIONE" DESC_TITOLO_DICHIARANTE,
    --BENEFICIARIO."COGNOME" COGNOME_BENEFICIARIO,
    --BENEFICIARIO."NOME" NOME_BENEFICIARIO,
    --TO_CHAR(BENEFICIARIO."DATA_NASCITA", 'DD/MM/YYYY') DATA_NASCITA_BENEFICIARIO,
    --COMUNE_BENEFICIARIO."DENOMINAZIONE" COMUNE_NASCITA_BENEFICIARIO,
    --COMUNE_BENEFICIARIO."COD_PROVINCIA" PROVINCIA_NASCITA_BENEFICIARIO,
    --BENEFICIARIO."COD_FISCALE" COD_FISCALE_BENEFICIARIO,
    TO_CHAR(ESENZIONI_CITTADINO."DATA_INIZIO", 'DD/MM/YYYY') DATA_INIZIO,
    TO_CHAR(ESENZIONI_CITTADINO."DATA_FINE", 'DD/MM/YYYY') DATA_FINE,
    ESENZIONI."COD_ESENZIONE",
    ESENZIONI."DESC_MOTIVO",
    ESENZIONI_CITTADINO."NOTA",
TO_CHAR(ESENZIONI_CITTADINO."DATA_RICHIESTA", 'DD/MM/YYYY') DATA_RICHIESTA

FROM
    "ESENRED_T_ESENZIONI_REDDITO" ESENZIONI_CITTADINO,
    "ESENRED_D_TIPI_ESENZIONI_REDDITO" ESENZIONI,
    "ESENRED_C_TITOLO_DICHIARANTE" TITOLO,
    "ESENRED_D_AZIENDASANITARIA" ASL
--    "ESENRED_T_CITTADINO" DICHIARANTE,
--    "ESENRED_T_CITTADINO" BENEFICIARIO,
--    "ESENRED_C_COMUNI" COMUNE_DICHIARANTE,
--    "ESENRED_C_COMUNI" COMUNE_BENEFICIARIO
WHERE
    ESENZIONI."COD_ESENZIONE" = ESENZIONI_CITTADINO."COD_ESENZIONE"
    AND ESENZIONI_CITTADINO."COD_NAZIONALE_ASL_RILASCIO" = ASL."ID_AZIENDA"
    AND ((ESENZIONI_CITTADINO."DATA_INSERT" BETWEEN ASL."DATA_ATTIVAZIONE" AND ESENZIONI_CITTADINO."DATA_INSERT") OR  ESENZIONI_CITTADINO."DATA_INSERT" IS NULL )
    AND TITOLO."COD_TITOLO" = ESENZIONI_CITTADINO."COD_TITOLO_DICHIARANTE"
--    AND ESENZIONI_CITTADINO."ID_CITTADINO_DICHIARANTE" = DICHIARANTE."ID_AURA"
--    AND ESENZIONI_CITTADINO."ID_CITTADINO_BENEFICIARIO" = BENEFICIARIO."ID_AURA"
--    AND DICHIARANTE."COD_ISTAT" = COMUNE_DICHIARANTE."COD_ISTAT"
--    AND BENEFICIARIO."COD_ISTAT" = COMUNE_BENEFICIARIO."COD_ISTAT"
    AND ESENZIONI_CITTADINO."ID_ESENZIONE" = $P{ID_ESENZIONE}]]>
	</queryString>
